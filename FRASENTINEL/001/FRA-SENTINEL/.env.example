# FRA-SENTINEL Environment Variables
# Copy this to .env for local development

# Flask Configuration
FLASK_APP=webgis/app.py
FLASK_ENV=development
SECRET_KEY=your-secret-key-here

# Database Configuration (Railway will provide these automatically)
DATABASE_URL=postgresql://user:password@localhost:5432/fra_atlas
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=fra_atlas
POSTGRES_USER=fra_user
POSTGRES_PASSWORD=password

# Redis Configuration (Railway will provide these automatically)
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Application Settings
DEBUG=True
LOG_LEVEL=info

# Railway will automatically set PORT
PORT=5000